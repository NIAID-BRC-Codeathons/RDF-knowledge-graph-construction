{
 "@context": {
    "@vocab": "https://schema.org/",
    "dct": "http://purl.org/dc/terms/"
  },
  "@type": "InfectiousDisease",
  "name": "MonkeyPox",
  "description": "An infectious disease",
  "sameAs": "https://en.wikipedia.org/wiki/Mpox",
  "infectiousAgentClass": {
    "@type": "InfectiousAgentClass",
    "name": "{Virus}",
    "description": "Infectious agent class for this disease."
  },

  "additionalProperty": [
    {
      "@type": "PropertyValue",
      "propertyID": "country",
      "value":  "{country}",
      "description": "Country where sample was collected."
    },
    {
      "@type": "PropertyValue",
      "propertyID": "taxon_id",
      "value":  "https://purl.uniprot.org/taxonomy/{tax_id}",
      "description": "Taxon ID for this infectious agent class."
    },
    {
      "@type": "PropertyValue",
      "propertyID": "run_accession",
      "value": "{run_accession}",
      "description": "Unique identifier assigned to a specific set of raw sequencing data files from a single sequencing event."
    },
    {
      "@type": "PropertyValue",
      "propertyID": "experiment_title",
      "value": "{experiment_title}",
      "description": "Brief, descriptive, human-readable title for a specific sequencing experiment."
    },
    {
      "@type": "PropertyValue",
      "propertyID": "sample_description",
      "value": "{description}",
      "description": "A human-readable description of the sample."
    }
  ]
}
