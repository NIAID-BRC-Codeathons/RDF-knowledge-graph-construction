PREFIX schema: <https://schema.org/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

SELECT ?subject ?edge ?object
WHERE {
    VALUES ?type { schema:InfectiousAgentClass schema:Place schema:Taxon}

    ?subject a ?type .
    ?object a ?type .
    ?subject ?edge ?object .

    FILTER (
        isIRI(?subject) &&
        !isBlank(?subject) &&
        isIRI(?object) &&
        !isBlank(?object)
    )
}
