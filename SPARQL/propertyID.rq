PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX schema: <https://schema.org/>

SELECT ?subject ?object ?propertyid ?value
WHERE {
	?subject a schema:InfectiousDisease .
  ?subject schema:additionalProperty ?object .
  ?object rdf:type ?otype .
  ?object schema:value ?value .
  ?object schema:propertyID ?propertyid .
}
